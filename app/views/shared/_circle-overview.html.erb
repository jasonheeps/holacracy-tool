<section class="circle-overview-container <%= locals[:visibility_class] %>" id="<%= locals[:dataset_id] %>">
    <ul>
      <% if @circle.parent_circle %>
        <li>
          <%= "Superkreis: #{@circle.parent_circle.title}" %>
        </li>
      <% end %>
      <li>
        Subkreise:
        <ul>
          <% @circle.sub_circles.each do |sc| %>
            <li><%= sc.title %></li>
          <% end %>
        </ul>
      </li>
      <li>
        Rollen:
        <ul>
          <% @circle.roles.each do |r| %>
            <li><%= r.title %></li>
          <% end %>
        </ul>
      </li>
    </ul>
</section>
